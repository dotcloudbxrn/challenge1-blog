  {{#each users}}
    <div class="user-content">
      <div class="user-profile">
        <a class="user-profile-picture" href="user/profile/{{this._id}}">
          <img class="profile-picture" src="{{this.avatar}}"/>
        </a>
        <a href="user/profile/{{this._id}}">{{this.username}}</a>
      </div>
      <div class="user-posts">
        Posts of {{this.firstName}} {{this.lastName}}
      </div>
    </div>
  {{/each}} 